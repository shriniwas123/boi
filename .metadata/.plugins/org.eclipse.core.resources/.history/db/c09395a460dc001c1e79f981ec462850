
public class ArrayDuplicate {

	public static void main(String[] args) {
		int count=0,i,j;
int a [] = {50,10,50,20,20,30,10,50,10};
for ( i =0; i<a.length; i++) {
	for( j =1; j<a.length-1;j++) {
		
	
	if (a[i]==a[j]) {
		count++;
	}

}
	if (count==1) {
		
		System.out.println(a[j]);
	}
	else {
		count=0;
	}
	
}


	}
}

